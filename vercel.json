{
  "version": 2,
  "builds": [
    {
      "src": "api/__init__.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/__init__.py"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "tu_openai_api_key_aqui",
    "ASSISTANT_ID": "tu_assistant_id_aqui"
  }
}